  "scripts": {
    "build-prod-webpack": "webpack --config webpack-prod.config.js",
    "build": "run-s install-functions build-prod-webpack",
    "build-dev": "cross-env NODE_ENV=development webpack --config webpack-dev.config.js",
    "install-functions": "node scripts/install-functions.mjs",
    "postinstall": "run-s install-functions",
    "start": "http-server -p 8888 --proxy http://localhost:8888? dist",
    "start-dev": "cross-env PORT=3004 NODE_ENV=development --env.AIRTABLE_API_KEY=keyEcVKWAxBoB9kuq webpack serve --config webpack-dev.config.js --mode development",
